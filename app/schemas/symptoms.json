{
    "$id": "symptoms.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "start": {
                "type": "string",
                "format": "date-time"
            },
            "end": {
                "type": "string",
                "format": "date-time"
            },
            "name": {
                "type": "string"
            },
            "severity": {
                "type": "string"
            },
            "userEntered": {
                "type": "boolean"
            },
            "source": {
                "type": "string"
            }
        },
        "required": ["start", "end", "name", "severity", "userEntered", "source"]            
    }
}